<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Background circle -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#E8F5E9"/>
            <size android:width="56dp" android:height="56dp"/>
        </shape>
    </item>

    <!-- Vertical bar of plus sign -->
    <item
        android:top="14dp"
        android:bottom="14dp"
        android:left="25dp"
        android:right="25dp">
        <shape android:shape="rectangle">
            <solid android:color="#2E7D32"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>

    <!-- Horizontal bar of plus sign -->
    <item
        android:top="25dp"
        android:bottom="25dp"
        android:left="14dp"
        android:right="14dp">
        <shape android:shape="rectangle">
            <solid android:color="#2E7D32"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>

</layer-list>